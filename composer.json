{
    "name": "aristosya/larafirebase",
    "description": "Laravel Firebase Cloud Messaging.With SendAll() method",
    "homepage": "https://https://github.com/Aristosya/larafirebase-with-sendAll",
    "license": "MIT",
    "authors": [
        {
            "name": "Gentrit Abazi, Aristosya",
            "email": "gentritabazi01@gmail.com, r.chernogorov.work@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GGInnovative\\Larafirebase\\": "src/"
        },
        "files" : [
            "src/Helpers/All.php"
        ]
    },
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/notifications": "^7.0|^8.0|^9.0|^10.0",
        "google/apiclient": "^2.12"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "GGInnovative\\Larafirebase\\Providers\\LarafirebaseServiceProvider"
            ]
        }
    }
}
